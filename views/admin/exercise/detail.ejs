<div class="container">
  <h2><%= exercise.name %></h2>
  
  <div>
    Description: <%= exercise.description %>
  </div>

  <div>
    Difficulty Level: <%= exercise.difficulty_level %>
  </div>

  <div>
    Duration: <%= exercise.duration %>
  </div>

  <div>
    Created At: <%= dayjs(exercise.createdAt).fromNow(true) %>
  </div>
  
  <div>
    Updated At: <%= dayjs(exercise.updatedAt).fromNow(true) %>
  </div>
  </div>
  
